{
    "name": "goten4/gtn-persist-base",
    "description": "Zend Framework 2 Module that provides Persistence basic functionality (trying to follow DDD principles)",
    "type": "library",
    "keywords": [
        "ddd",
        "module",
        "zf2"
    ],
    "authors": [
        {
            "name": "Emmanuel Bouton",
            "email": "goten4@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-loader": "2.3.*",
        "zendframework/zend-modulemanager": "2.3.*",
        "zendframework/zend-servicemanager": "2.3.*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "GtnPersistBase\\": "src/",
            "GtnPersistBaseTest\\": "tests/"
        }
    }
}