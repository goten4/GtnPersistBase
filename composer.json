{
    "name": "goten4/zf-persistence-base",
    "description": "Zend Framework 2 Module that provides Persistence basic functionality (trying to follow DDD principles)",
    "type": "library",
    "keywords": [
        "ddd",
        "module",
        "zf2"
    ],
    "authors": [
        {
            "name": "Emmanuel Bouton",
            "email": "goten4@gmail.com",
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-config": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-servicemanager": "2.*",
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "ZfPersistenceBase\\": "src/",
            "ZfPersistenceBaseTest\\": "tests/"
        }
    }
}